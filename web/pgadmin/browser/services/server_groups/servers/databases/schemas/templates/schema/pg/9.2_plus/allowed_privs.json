{# List of allowed privileges for PostgreSQL 9.2 or later #}
{#
    Format for allowed privileges is:
    "acl_col": {
        "type": "name",
        "acl": [...]
    }
#}
{
    "nspacl": {
        "type": "SCHEMA",
        "acl": ["C", "U"]
    },
    "deftblacl": {
        "type": "TABLE",
        "acl": ["r", "a", "w", "d", "D", "x", "t"]
    },
    "defseqacl": {
        "type": "SEQUENCE",
        "acl": ["U", "r", "a"]
    },
    "deffuncacl": {
        "type": "FUNCTION",
        "acl": ["X"]
    },
    "deftypeacl": {
        "type": "TYPE",
        "acl": ["U"]
    }
}
