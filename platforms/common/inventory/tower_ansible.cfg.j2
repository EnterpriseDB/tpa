[defaults]
roles_path = /opt/EDB/TPA/roles
collections_paths = /opt/EDB/TPA/tpa-venv/collections
filter_plugins = /opt/EDB/TPA/lib/filter_plugins
test_plugins = /opt/EDB/TPA/lib/test_plugins
action_plugins =/opt/EDB/TPA/lib/action_plugins
lookup_plugins = /opt/EDB/TPA/lib/lookup_plugins
library = /opt/EDB/TPA/library
nocows = 1
nocolor = 0
gathering = explicit
private_role_vars = yes
command_warnings = False
localhost_warnings = False
deprecation_warnings = False
squash_actions =
timeout = 120
forks = 16
retry_files_enabled = no
retry_files_save_path = ~/.ansible-retry
inject_facts_as_vars = true
force_valid_group_names=ignore
vault_password_file=vault/vault_pass.txt

[inventory]
unparsed_is_failed = true
any_unparsed_is_failed = true
enable_plugin = aws_ec2, host_list, auto, script, yaml, ini, toml

[ssh_connection]
ssh_args = -o ControlMaster=auto -o ControlPersist=60s -o ControlPath=/tmp/ansible-ssh-%h-%p-%r
pipelining = True
