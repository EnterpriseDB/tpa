# Please do not edit this file. Your changes will be overwritten. Put
# your configuration overrides into conf.d/9999-override.conf instead.

autovacuum = on

autovacuum_max_workers = {{ autovacuum_max_workers|default(3) }}
log_autovacuum_min_duration = {{ log_autovacuum_min_duration|default('5s') }}

autovacuum_freeze_max_age = {{ autovacuum_freeze_max_age|default(xxx) }}
autovacuum_multixact_freeze_max_age = {{ autovacuum_multixact_freeze_max_age|default(xxx) }}

autovacuum_naptime = {{ autovacuum_naptime|default(xxx) }}
autovacuum_vacuum_threshold = {{ autovacuum_vacuum_threshold|default(xxx) }}
autovacuum_analyse_threshold = {{ autovacuum_analyse_threshold|default(xxx) }}
autovacuum_vacuum_scale_factor = {{ autovacuum_vacuum_scale_factor|default(xxx) }}
autovacuum_analyze_scale_factor = {{ autovacuum_analyze_scale_factor|default(xxx) }}

autovacuum_vacuum_cost_delay = {{ autovacuum_vacuum_cost_delay|default('20ms') }}
autovacuum_vacuum_cost_limit = {{ autovacuum_vacuum_cost_limit|default(-1) }}

vacuum_cost_delay = {{ vacuum_cost_delay|default(0) }}
vacuum_cost_limit = {{ vacuum_cost_limit|default(200) }}
